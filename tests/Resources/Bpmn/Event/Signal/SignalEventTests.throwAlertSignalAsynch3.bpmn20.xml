<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
	xmlns:extension="http://activiti.org/bpmn" 
	targetNamespace="Examples">
	
	<signal id="alertSignal3" name="alert3" />

	<process id="throwSignal3" isExecutable="true">

		<startEvent id="start" />

		<sequenceFlow sourceRef="start" targetRef="signalEvent" />

		<intermediateThrowEvent id="signalEvent" name="Alert" >
			<signalEventDefinition signalRef="alertSignal3" extension:async="true" />			
		</intermediateThrowEvent>

		<sequenceFlow sourceRef="signalEvent" targetRef="end" />

		<endEvent id="end" />

	</process>

</definitions>
