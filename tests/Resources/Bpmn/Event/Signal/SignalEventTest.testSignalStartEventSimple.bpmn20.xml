<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
	xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
	xmlns:extension="http://activiti.org/bpmn" 
	targetNamespace="Examples">

	<signal id="alertSignal" name="alert simple" />

	<process id="startBySignal1_02" isExecutable="true">

		<startEvent id="start">
			<signalEventDefinition signalRef="alertSignal" />
		</startEvent>

		<sequenceFlow sourceRef="start" targetRef="task" />

		<userTask id="task" name="start process task simple"/>

		<sequenceFlow sourceRef="task" targetRef="end" />

		<endEvent id="end" />

	</process>

	

</definitions>
